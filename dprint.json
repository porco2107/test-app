{
  "incremental": true,
  "lineWidth": 100,
  "useTabs": false,
  "indentWidth": 2,
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.92.0.wasm",
    "https://plugins.dprint.dev/json-0.17.3.wasm",
    "https://plugins.dprint.dev/markdown-0.16.4.wasm",
    "https://plugins.dprint.dev/toml-0.6.1.wasm"
  ],
  "includes": ["**/*.{ts,tsx,js,jsx,json,md,markdown,toml}"],
  "excludes": ["dist", "build", "coverage", ".pnpm", "node_modules"]
}
